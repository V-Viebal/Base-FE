{
	"name": "web",
	"version": "0.0.8",
	"description": "Web",
	"authors": "Web Team",
	"private": true,
	"scripts": {
		"sass-lint": "node --max-old-space-size=4096 ./node_modules/sass-lint/bin/sass-lint.js",
		"pug-lint": "node --max-old-space-size=4096 ./node_modules/pug-lint/bin/pug-lint",
		"version-patch": "npm --no-git-tag-version version patch && node store-version.js",
		"hmr": "nx serve --host localhost --configuration=hmr",
		"local": "nx serve --host localhost --configuration=local",
		"prod": "nx serve --host localhost --configuration=production",
		"aot": "nx serve --host localhost --configuration=aot",
		"dev": "nx serve --host localhost --configuration=dev",
		"build": "nx build --configuration=local --output-path ./dist/web",
		"build-prod": "nx build --configuration=production && node compress.js",
		"build-prod-gzip": "nx build --configuration=production && gzipper c ../../dist/production --exclude gz --output-file-format [filename].[ext].gz ../../dist/compressed"
	},
	"engines": {
		"node": ">=20.15.0",
		"npm": ">=10.7.0"
	},
	"dependencies": {
		"@ctrl/ngx-emoji-mart": "^9.2.0",
		"@editorjs/checklist": "^1.5.0",
		"@editorjs/code": "^2.8.0",
		"@editorjs/delimiter": "^1.3.0",
		"@editorjs/editorjs": "^2.30.2",
		"@editorjs/header": "^2.7.0",
		"@editorjs/inline-code": "^1.4.0",
		"@editorjs/list": "^1.8.0",
		"@editorjs/marker": "^1.3.0",
		"@editorjs/paragraph": "^2.10.0",
		"@editorjs/quote": "^2.5.0",
		"@editorjs/raw": "^2.4.0",
		"@editorjs/table": "^2.2.2",
		"@editorjs/underline": "^1.1.0",
		"@fortawesome/fontawesome-free": "^6.6.0",
		"@types/swiper": "^6.0.0",
		"angular-resizable-element": "^7.0.2",
		"angular-svg-round-progressbar": "^12.0.0",
		"angularx-qrcode": "^17.0.1",
		"editorjs-drag-drop": "^1.1.13",
		"editorjs-parser": "^1.5.3",
		"editorjs-text-alignment-blocktune": "^1.0.3",
		"editorjs-undo": "^2.0.23",
		"file-saver": "^2.0.5",
		"html-to-image": "^1.11.11",
		"image-conversion": "^2.1.1",
		"leader-line-new": "^1.1.9",
		"libphonenumber-js": "^1.11.4",
		"ng-lazyload-image": "^9.1.3",
		"ng-recaptcha": "^13.2.1",
		"ngx-color": "^5.1.3",
		"ngx-quill-upload": "^2.0.0",
		"primeicons": "^7.0.0",
		"primeng": "^17.18.9",
		"quill": "2.0.0-rc.5",
		"quill-magic-url": "^2.0.2",
		"quill-mention": "^3.4.1",
		"quill-table": "^1.0.0",
		"quill-table-ui": "^1.0.7",
		"resize-observer-polyfill": "^1.5.1",
		"swiper": "^11.1.11",
		"tslib": "^2.4.0"
	},
	"devDependencies": {
		"@angularclass/hmr": "^3.0.0",
		"@babel/core": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"@babel/preset-flow": "^7.0.0",
		"@types/file-saver": "^2.0.7",
		"@types/lodash": "^4.17.6",
		"sass-lint": "^1.13.1",
		"ts-mixer": "^6.0.3"
	}
}
