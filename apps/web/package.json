{
	"name": "csr",
	"version": "0.0.0",
	"authors": "Mons",
	"private": true,
	"scripts": {
		"sass-lint": "node --max-old-space-size=4096 ./node_modules/sass-lint/bin/sass-lint.js",
		"pug-lint": "node --max-old-space-size=4096 ./node_modules/pug-lint/bin/pug-lint",
		"version-patch": "npm --no-git-tag-version version patch && node store-version.js",
		"aot": "nx serve-ssr --host localhost --configuration=aot",
		"dev": "nx serve-ssr --configuration=develop",
		"prod": "nx serve-ssr --host localhost --configuration=production",
		"build": "nx build --configuration=develop && nx server --configuration=develop",
		"build-prod": "nx build --configuration production && nx server --configuration=production",
		"prerender": "nx prerender --configuration=develop",
		"prerender-prod": "nx prerender --configuration=production",
		"build-prerender": "nx run build && nx run prerender && node ../../dist/web/server/main.js"
	},
